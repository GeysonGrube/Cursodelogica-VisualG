algoritmo "Exer10"
//  
//  

var

    n1, cont, mult : inteiro

inicio

      cont := 1
      mult := 0
      enquanto cont <= 10 faca
               para n1 de 1 ate 10 faca
                    mult := n1 * cont
                    escreval (cont," x",n1," =",mult)
               fimpara
      cont := cont + 1
      fimenquanto

fimalgoritmo